#include  <iostream>
using namespace std;

int main()
{
  int marks;

  cout<<"Please enter the marks to calculate grades : ";
  cin>>marks;

  if(marks>90)
  {
    cout<"A";
  }
  if(marks<90 && marks>80)
  {
    cout<"B";
  }
  if(marks<80 && marks>70)
  {
    cout<<"C";
  }
  else 
  {
    cout<<"Pass";
  }
